{
    "rules": {
        "class-name": true,
        "curly": true,
        "eofline": true,
        "indent": [true, 2],
        "interface-name": false,
        "jsdoc-format": true,
        "member-ordering": [
            "static-before-instance",
            "variables-before-functions"
        ],
        "no-arg": true,
        "no-bitwise": true,
        "no-consecutive-blank-lines": true,
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-shadowed-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-require-imports": false,
        "no-trailing-whitespace": true,
        "no-unused-expression": true,
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "one-line": [
            "check-catch",
            "check-else",
            "check-open-brace",
            "check-whitespace"
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "radix": true,
        "semicolon": true,
        "switch-default": true,
        "trailing-comma": false,
        "triple-equals": true,
        "typedef": [
            "call-signature",
            "parameter",
            "property-declaration",
            "variable-declaration",
            "member-variable-declaration"
        ],
        "whitespace": [
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-type",
            "check-typecast"
        ]
    }
}